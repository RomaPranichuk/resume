<div>
  <main>
    <header>
      <h1>{{name}}</h1>
    </header>
    <section>
      <b>Desc: </b><em>{{description}}</em>
      <br />
      <br />
      <div>
        <h2><span
            class='badge text-bg-primary'
          >History</span></h2>
        {{#with history}}
          <strong>Javascript was founded in
            {{year}}</strong>
          <br />
          <br />
          <b>Founded by: </b>{{founder}}
        {{/with}}
      </div>
      <br />
      <br />
      <div>
        <h3><span
            class='badge text-bg-info'
          >Types</span></h3>
        <ul>
          {{#each types}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
      <br />
      <div>
        <h3><span class='badge text-bg-danger'>Syntax</span>
        </h3>
        <br />
        {{#with syntax}}
          <b>Variables</b>
          <ul>
            {{#each variables}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
          <b>Functions</b>
          <ul>
            {{#each functions}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
          <b>Conditionals</b>
          <ul>
            {{#each conditionals}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
          <b>Loops</b>
          <ul>
            {{#each loops}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
          <b>Classes</b>
          <ul>
            {{#each classes}}
              <li>{{this}}</li>
            {{/each}}
          </ul>
        {{/with}}
      </div>
      <br />
      <div>
        <h3><span
            class='badge text-bg-warning'
          >Features</span>
        </h3>
        <ul>
          {{#each features}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
      <br />
      <div>
        <h3><span
            class='badge text-bg-success'
          >Libraries</span>
        </h3>
        <ul>
          {{#each libraries}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
      <br />
      <div>
        <h3><span class='badge text-bg-primary'>Tools</span>
        </h3>
        <ul>
          {{#each tools}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
      <br />
      <div>
        <h3><span
            class='badge text-bg-primary'
          >Comunities</span>
        </h3>
        <ul>
          {{#each community}}
            <li>{{name}} - {{type}}</li>
          {{/each}}
        </ul>
      </div>
    </section>
  </main>
</div>
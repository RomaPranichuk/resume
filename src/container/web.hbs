<div>
	{{#with web}}
	<main>
		<h2>JavaScript</h2>
		{{#each languages}}
		{{#if functions}}
		{{! Replace `functions` with other objects --}}
		{{#with this}}
		<section>
			<div>
				<h3>{{name}}</h3>
				<strong>Version: </strong>{{version}}
				<br>
				<br>
				<em>Desc: {{description}}</em>
			</div>
			<br>
			<h3>Functions</h3>
			{{#each functions}}
			<div>
				<ul>
					<li>
						<b>{{name}}</b>
						-
						<em>{{description}}</em>
						<br>
						<div>
							<h4>Parameters</h4>
							{{#each parameters}}
							<b>Name: {{name}}</b>
							<br>
							<br>
							<b>Type: </b>{{type}}
							<br>
							<br>
							<b>Desc: </b><em>{{description}}</em>
							{{/each}}
						</div>
					</li>
				</ul>
			</div>
			{{/each}}
		</section>
		{{/with}}
		{{/if}}
		{{/each}}
	</main>
	{{/with}}
</div>
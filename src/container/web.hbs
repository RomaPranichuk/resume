<div>
	{{#with web}}
	<main>
		<header>
			<h1>Web Languages</h1>
		</header>
		<h2>HyperText Markup Language</h2>
		{{#each languages}}
		{{#if elements}}
		{{! Replace `elements` with other objects --}}
		{{#with this}}
		<section>
			<div>
				<h3>{{name}}</h3>
				<strong>Version: </strong>{{version}}
				<br>
				<br>
				<em>Desc: {{description}}</em>
			</div>
			<br>
			<h3>Elements</h3>
			{{#each elements}}
			<div>
				<ul>
					<li>
						<b>{{name}}</b>
						-
						<em>{{description}}</em>
						<br>
						<div>
							<h4>Attributes</h4>
							{{#each attributes}}
							{{#if name includeZero='class'}} <br>
							<b>{{name}}</b>
							-
							<em>{{description}}</em>
							{{/if}}
							{{/each}}
						</div>
					</li>
				</ul>
			</div>
			{{/each}}
		</section>
		{{/with}}
		{{/if}}
		{{/each}}
	</main>
	{{/with}}
</div>


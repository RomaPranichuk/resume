<div>
	{{#with web}}
	<main>
		<h2>Cascading Style Sheet</h2>
		{{#each languages}}
		{{#if properties}}
		{{! Replace `properties` with other objects --}}
		{{#with this}}
		<section>
			<div>
				<h3>{{name}}</h3>
				<strong>Version: </strong>{{version}}
				<br>
				<br>
				<em>Desc: {{description}}</em>
			</div>
			<br>
			<h3>Properties</h3>
			{{#each properties}}
			<div>
				<ul>
					<li>
						<b>{{name}}</b>
						-
						<em>{{description}}</em>
						<br>
						<div>
							<h4>Values</h4>
							{{#each values}}
							{{#if value includeZero='blue'}} <br>
							<b>{{value}}</b>
							-
							<em>{{description}}</em>
							{{/if}}
							{{/each}}
						</div>
					</li>
				</ul>
			</div>
			{{/each}}
		</section>
		{{/with}}
		{{/if}}
		{{/each}}
	</main>
	{{/with}}
</div>


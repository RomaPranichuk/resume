<header>
  <div class='row gx-5'>
    {{#with navigation}}
      <div class='row'>
        <section class='col-5'>
          {{#each links}}
            <a
              href='{{href}}'
              class='btn btn-link'
            >{{text}}</a>
          {{/each}}
        </section>
        <section class='col-2'>
          {{#with button}}
            <a
              href='{{href}}'
              class='btn btn-primary'
            >{{text}}</a>
          {{/with}}
        </section>
      </div>
    {{/with}}
    <hr />
    <section class='col-12'>
      {{#with header}}
        <h1>{{title}}</h1>
        <p>{{description}}</p>
        {{#with button}}
          <a
            href='{{link}}'
            class='btn btn-primary'
          >{{text}}</a>
        {{/with}}
      {{/with}}
      <hr />
    </section>
  </div>
</header>

<main>
  <div class='row gy-2'>
    {{#with city}}
      <h2>{{title}}</h2>
      <section class='row gy-3'>
        {{#each cards}}
          <div class='col-5'>
            <div class='card'>
              {{#with picture}}
                <img
                  class='card-img-top'
                  src='{{src}}'
                  alt='{{alt}}'
                />
              {{/with}}
              <div class='card-body'>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
                {{#with button}}
                  <a
                    href='{{href}}'
                    class='btn btn-primary'
                  >{{text}}</a>
                {{/with}}
              </div>
            </div>
          </div>
        {{/each}}
        <hr />
      </section>
    {{/with}}
    {{#with winner}}
      <h2>{{title}}</h2>
      <section class='col-12'>
        {{#with table}}
          <table class='table table-bordered'>
            <thead>
              <tr>
                {{#each head}}
                  <th>{{this}}</th>
                {{/each}}
              </tr>
            </thead>
            <tbody>
              {{#each user}}
                <tr>
                  <th>{{place}}</th>
                  <td>{{name}}</td>
                  <td>{{surname}}</td>
                  <td>{{nickname}}</td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        {{/with}}
      </section>
      <hr />
    {{/with}}
    {{#with gallery}}
      <h2>{{title}}</h2>
      <section class='row'>
        {{#each cards}}
          <div class='col-4'>
            <div class='card'>
              <div class='card-header'>
                <div>{{name}}</div>
              </div>
              <ul class='list-group list-group-flush'>
                {{#each items}}
                  <li class='list-group-item'>{{this}}</li>
                {{/each}}
              </ul>
            </div>
          </div>
        {{/each}}
      </section>
      <hr />
    {{/with}}
    {{#with relaxion}}
      <div class='display-3'>
        <b>{{title}}</b>
      </div>
      <section class='row gy-3'>
        {{#each cards}}
          <div class='col-5'>
            <div class='card text-bg-secondary'>
              <div class='card-header'>
                <div>{{header}}</div>
              </div>
              <div class='card-body'>
                <h5>{{title}}</h5>
                <p>{{description}}</p>
              </div>
            </div>
          </div>
        {{/each}}
      </section>
      <hr />
    {{/with}}
    {{#with rest}}
      <section class='row gy-3'>
        {{#each cards}}
          <div class='col-6'>
            <div class='card'>
              <div class='card-header text-center'>
                <div>{{header}}</div>
              </div>
              <div class='card-body text-center'>
                <h6>{{title}}</h6>
                <p>{{description}}</p>
                <button
                  class='btn btn-primary'
                >{{button}}</button>
              </div>
              <div class='card-footer text-center'>
                <div>{{footer}}</div>
              </div>
            </div>
          </div>
        {{/each}}
      </section>
      <hr />
    {{/with}}
    {{#with proposition}}
      <div class='display-2'>
        <b>{{title}}</b>
      </div>
      <section class='row gy-3'>
        {{#each card}}
          <div class='col-5'>
            <div class='card text-bg-warning'>
              <div class='card-header'>
                <div>{{header}}</div>
              </div>
              <div class='card-body'>
                <h6>{{title}}</h6>
                <p>{{description}}</p>
              </div>
            </div>
          </div>
        {{/each}}
      </section>
    {{/with}}
  </div>
  <hr />
</main>

<footer>
  <div>
    {{#with footer}}
      <h5>{{title}}</h5>
      <div class='row gy-5 gx-5'>
        <section class='col-2'>
          {{#each navigation}}
            <div class='mb'>
              <a
                href='{{href}}'
                class='btn btn-link'
              >{{text}}</a>
            </div>
          {{/each}}
        </section>
        <section class='col-3'>
          {{#each socialMedia}}
            <div class='p-1'>
              <a
                href='{{link}}'
                class='btn btn-outline-dark'
              >{{name}}</a>
            </div>
          {{/each}}
        </section>
        <section class='col-3'>
          {{#each partners}}
            <div class='p-1'>
              <a
                href='{{link}}'
                class='btn btn-outline-info'
              >{{name}}</a>
            </div>
          {{/each}}
        </section>
      </div>
    {{/with}}
  </div>
</footer>